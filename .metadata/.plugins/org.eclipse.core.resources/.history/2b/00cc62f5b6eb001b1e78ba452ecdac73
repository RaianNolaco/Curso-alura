
public class TestaCondicional {
	
	public static void main(String[] args) {
		
		System.out.println("Testeando condicionais");
		
		int idade = 12;
		
		if(idade >= 18){
			
			System.out.println("Você é de maior");
			
			
		}else {
			
			System.out.println("Infelizmente você não pode entrar");
			
		}
		
	}

}
